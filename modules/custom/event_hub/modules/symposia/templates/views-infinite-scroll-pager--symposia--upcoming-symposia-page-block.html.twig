{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}

{% if items.next %}
<ul{{ attributes }}>
  <li class="pager__item coh-col-xl-8">
    <a class="button coh-style-a-button-alternative" href="{{ items.next.href }}" title="{{ 'Load more items'|t }}" rel="next">
      {{ options.button_text }}
    </a>
  </li>
</ul>
{% endif %}
